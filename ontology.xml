        <rdfs:subClassOf rdf:resource="http://bim.aalto.fi/DiCtion/Ontology/LevelScheme"/>
        <rdfs:comment xml:lang="en">A specific level scheme of the smartness capability.</rdfs:comment>
    </owl:Class>
