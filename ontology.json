  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Material refers to the physical substance that participates in the construction projectas the input of a transformation process, such as cement."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://w3id.org/digitalconstruction/PhysicalEntity"
  } ]
}, {
  "@id" : "http://w3id.org/digitalconstruction/PhysicalEntity",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Refers to any object that has physical dimensions and a position."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://w3id.org/digitalconstruction/DomainObject"
  } ]
}, {
  "@id" : "http://w3id.org/digitalconstruction/Resource",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Resource concept refers to the objects within a construction project that participate in value-adding transformations but that are not ultimately transformed themselves (such as labors and equipments)."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://w3id.org/digitalconstruction/DomainObject"
  } ]
}, {
  "@id" : "http://w3id.org/digitalconstruction/SmartnessLevel",
